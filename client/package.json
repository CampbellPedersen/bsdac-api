{
  "name": "bsdapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.19.2",
    "bootstrap": "4.5.0",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "3.7.5"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.48",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "axios-mock-adapter": "1.18.1",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "react-scripts": "3.4.1",
    "ts-jest": "25.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "verbose": true,
    "rootDir": "./",
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.ts?$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node"
  }
}
