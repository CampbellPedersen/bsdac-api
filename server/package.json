{
  "name": "bsdac-api",
  "version": "1.0.0",
  "description": "An API built in Typescript for retrieving resources dedicated to Big Smash Day and Cube.",
  "main": "server.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CampbellPedersen/bsdac-api.git"
  },
  "keywords": [],
  "author": "Campbell Pedersen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CampbellPedersen/bsdac-api/issues"
  },
  "homepage": "https://github.com/CampbellPedersen/bsdac-api#readme",
  "devDependencies": {
    "@types/aws-sdk": "2.7.0",
    "@types/express": "4.17.4",
    "@types/jest": "25.2.1",
    "@types/multer": "1.4.3",
    "@types/node": "13.11.0",
    "@types/supertest": "2.0.6",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "eslint": "6.8.0",
    "jest": "25.2.7",
    "rimraf": "3.0.2",
    "supertest": "3.3.0",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "aws-sdk": "2.814.0",
    "express": "4.17.1",
    "multer": "1.4.2",
    "uuid": "8.1.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "test": "jest",
    "build": "rimraf ./build && tsc",
    "start": "docker-compose up -d",
    "stop": "docker-compose down"
  },
  "jest": {
    "verbose": true,
    "rootDir": "./",
    "globalSetup": "<rootDir>/src/jest-setup.ts",
    "globalTeardown": "<rootDir>/src/jest-teardown.ts",
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.ts?$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node"
  }
}
